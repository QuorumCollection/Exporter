{
	"name": "quorum/exporter",
	"description": "Export Data as Spreadsheets",
	"require": {
		"maennchen/zipstream-php": "~2.1",
		"ext-SPL": "*",
		"ext-mbstring": "*",
		"ext-dom": "*",
		"ext-json": "*",
		"php": ">=7.4"
	},
	"require-dev": {
		"donatj/drop": "*",
		"phpunit/phpunit": "4.* | ~8.5 | ^9.0",
		"friendsofphp/php-cs-fixer": "^3.16",
		"squizlabs/php_codesniffer": "^3.6",
		"corpus/coding-standard": "^0.7.0",
		"donatj/mddoc": "^0.9.0"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Jesse Donat",
			"email": "donatj@gmail.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Quorum\\Exporter\\": "src/"
		}
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
